<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="table-responsive">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Nombre</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var objeto in Model)
                    {
                    <tr>
                        <td>@objeto.Id</td>
                        <td>@objeto.Nombre</td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>